.row.gym-account-info
  %p.page-title GYM ACCOUNT
  .row.account-box
    .row
      %p.container-title GYM AND LOCATION INFO
    .span13.control-box
      .row.first-row
        .span4
          .control-group
            .controls
              %input#first-name{:placeholder => "Jogn", :type => "text"}/
        .span4
          .control-group
            .controls
              %input#last-name{:placeholder => "Doe", :type => "text"}
        .span4
          .control-group
            .controls
              %input#email-address{ :placeholder => "jdoe@email.com", :type => "email" }
      .row.second-row
        .span8
          .control-group
            .controls
              %input#street-name{:placeholder => "123 Street", :type => "text"}/
        .span4
          .control-group
            .row
              .span3
                %input#first-name{:placeholder => "City", :type => "text"}/
              .span1
                .custom-select
                  .select-arrow
                  %select
                    %option ST
                    %option US
      .row.third-row
