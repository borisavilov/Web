            .span1{:style=>"margin-left: 0px;"}
              %input{:placeholder => "20", :type => "text"}

    .row.account-box
      .row
        %p.container-title BANKING INFO
      .span13.control-box.banking-info-box
        .row
          .span8
            .control-group
              .controls
                %input#full-name{:placeholder => "name on card", :type => "text"}/
          .span4
            .control-group
              .row
                .span2
                  %label TYPE OF ACCOUNT
                .span2
                  .custom-select
                    .select-arrow
                    %select
                      %option 00
                      %option 01
                      %option 02
                      %option 03
                      %option 04
                      %option 05
        .row
          .span6
            .control-group
              .controls
                %input#phone-number{:placeholder => "0000000", :type => "text"}/
          .span6
            .control-group
              .controls
                %input#cvv-field{:placeholder => "0000000", :type => "text"}
