      .row
        .span2
          %p.container-title PRICING
        .span4
          %label.pricing-label Blurb about pricing
      .span13.pricing-box
        .span4.pricing-col
          .controls.controls-row
            .span1.img-box{:style=>"width:100px;"}
              =image_tag('one.png')
            .span1{:style=>"width:50px;"}
              %p{:style=>"font-size: 40px; margin-left: 20px; margin-top: 33px;"} $
            .span1{:style=>"margin-left: 0px;"}
